import java.text.ParseException;


public class IOsample  {

		public static void Run() throws Exception {
			try{
				InputStream in = System.in;
				InputStreamReader inReader = new InputStreamReader(in);
			} catch ( IOException e ) {
			} catch ( ParseException pe) { }
			
			
		}
}
